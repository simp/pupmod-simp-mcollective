{
  "name": "simp-mcollective",
  "version": "2.3.3",
  "author": "SIMP Team",
  "summary": "Installs, configures, and manages MCollective agents, clients, and middleware of an MCollective cluster.",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-mcollective",
  "project_page": "https://github.com/simp/pupmod-simp-mcollective",
  "issues_url": "https://github.com/simp/pupmod-simp-mcollective/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ],
  "description": "SIMP MCollective Puppet Module",
  "dependencies": [
    {
      "name": "richardc/datacat",
      "version_requirement": ">= 0.6.2 < 1.0.0"
    },
    {
      "name": "puppetlabs/java_ks",
      "version_requirement": ">= 1.4.0 < 2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
